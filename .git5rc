[sync]
tap-project=asfe_all
tap-options=use-green
#sync-at=head
sync-at=green

[mail]
tap-project=asfe_all,mercury
#tap-options=use-green,detach,email
fast-export=True
suppress-diffs=True
citc=True

[export]
tap-project=asfe_all,mercury
tap-options=use-green,detach,email
fast-export=True
citc=True
commit=True

[submit]
#tap-project=asfe_all,mercury
#tap-options=use-green
#submit-queue=True
fast-export=True

[diff]
#command = difftool

[start]
sync-at=head

[alias]
tap = export --submit-queue --tap-options=detach,no-email
tap-mail = export --submit-queue --tap-options=detach,email
head = sync '#head'
rerun = submit --tap-options='rerun=last'
difftool = diff -c difftool
