{
  "editor.fontFamily": "'Iosevka Slab Medium', 'JetBrains Mono', 'Iosevka SS08', 'Iosevka Term Slab Medium', 'monospace', 'Droid Sans Fallback'",
  // "editor.fontFamily": "PragmataPro Liga",
  "editor.fontSize": 12,
  "editor.rulers": [
    100
  ],
  "editor.accessibilitySupport": "off",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.minimap.maxColumn": 100,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "boundary",
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.wordWrapColumn": 100,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.enableSmartCommit": true,
  "keyboard.dispatch": "keyCode",
  "vim.textwidth": 100,
  "workbench.colorTheme": "Gruvbox Dark Medium",
  // See https://github.com/jdinhify/vscode-theme-gruvbox/blob/master/colors.txt
  "workbench.colorCustomizations": {
    // "editorGutter.background": "#3c3836",
    "tab.activeBorder": "#FF0000",
    "panelTitle.activeBorder": "#ff0000",
    "tab.activeBorderTop": "#FF0000",
    // "tab.activeBackground": "#ffFF00",
    // "tab.unfocusedActiveBackground": "#FF0000"
  },
  "editor.codeActionsOnSave": {
    // "source.fixAll.eslint": true
  },
  "linkme.rules": [
    {
      "pattern": "([a-z]{1,4})/([^)]+)",
      "url": "https://\\1/\\2"
    }
  ],
  "files.associations": {"*.tmpl": "html"},
  "[cpp]": {
    "editor.tabSize": 2
  },
  "[rust]": {
    "editor.defaultFormatter": "matklad.rust-analyzer",
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
  },
  "[yaml]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true
  },
  "[toml]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true
  },
  "[java]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
    // "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    // "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    // "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[proto3]": {
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 80,
    "editor.quickSuggestions": false,
    "editor.rulers": [
      80
    ],
    "editor.minimap.maxColumn": 80
  },
  "prettier.eslintIntegration": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "files.autoSave": "off",
  "editor.formatOnSave": true,
  "bazel.buildifierFixOnFormat": true,
  "markdownlint.run": "onSave",
  // "rust-analyzer.serverPath": "rust-analyzer",
  "rust-analyzer.trace.server": "messages",
  "editor.tokenColorCustomizations": {
    "comments": {
      // "foreground": "#FF0000",
      // "fontStyle": "bold"
    }
  },
  "files.watcherExclude": {
    "**/.cargo/**": true,
    "**/.git/**": true,
    "**/bazel-*": true,
    "**/bazel-*/**": true,
    "**/cargo-cache/**": true,
    "**/target/**": true
  },
  "rust-analyzer.completion.addCallArgumentSnippets": false,
  "rust-analyzer.completion.addCallParenthesis": false,
  "rust-analyzer.completion.postfix.enable": false,
  "terminal.integrated.fontSize": 12,
  "files.exclude": {
    "**/bazel-*": true,
    "**/cargo-cache/**": true
  },
  "rust-analyzer.inlayHints.typeHints": false,
  "rust-analyzer.inlayHints.parameterHints": false,
  "scm.diffDecorationsGutterWidth": 5,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.trace.extension": true,
  "prettier.requireConfig": true,
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.scrollback": 10000,
  "remote.autoForwardPorts": false,
  "remote.restoreForwardedPorts": false,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.advanced": {
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false,
    "toml": false
  },
  "go.toolsManagement.autoUpdate": true,
  "editor.bracketPairColorization.enabled": true,
  "git.confirmSync": false,
  "security.workspace.trust.untrustedFiles": "open",
  // "rust-analyzer.cargo.target": "wasm32-unknown-unknown"
}
